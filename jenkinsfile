pipeline {
  agent any

    stages {
      stage('Scm Checkout') {
        steps {
          git branch: 'main', url: 'https://github.com/rahatdevops/java-jenkins.git'
            }
         }
      stage('Build App') {
        steps {
          sh 'mvn clean package'
            }
          }      
   }			                   
}    

